{
  "name": "xenioushk/kb-tab-for-woocommerce",
  "description": "Tthis Addon allows you to convert your knowledge base posts in to WooCommerce product Knowledgebase items.",
  "type": "project",
  "autoload": {
    "psr-4": {
      "KTFWC\\": "includes/"
    }
  },
  "require-dev": {
    "wp-coding-standards/wpcs": "^3.1",
    "phpcompatibility/phpcompatibility-wp": "2.1",
    "dealerdirect/phpcodesniffer-composer-installer": "^1.0",
    "squizlabs/php_codesniffer": "^3.10"
  },
  "config": {
    "allow-plugins": {
      "composer/installers": true,
      "dealerdirect/phpcodesniffer-composer-installer": true,
      "johnpbloch/wordpress-core-installer": true
    }
  },
  "authors": [
    {
      "name": "Mahbub Alam Khan",
      "email": "hkhan.cse@gmail.com"
    }
  ],
  "minimum-stability": "stable"
}
